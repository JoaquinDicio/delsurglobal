---
import "../styles/global.css";
import Header from "../components/astro/Header.astro";
import Home from "../components/astro/Home.astro";
import ImportAndExport from "../components/astro/ImportAndExport.astro";
import Distribution from "../components/astro/Distribution.astro";
import Products from "../components/astro/Products.astro";
import Contact from "../components/astro/Contact.astro";
import Footer from "../components/astro/Footer.astro";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet" />
    <title>Astro</title>
  </head>
  <body>
    <Header />
    <Home />
    <ImportAndExport />
    <Distribution />
    <Products />
    <Contact />
    <Footer />
  </body>

  <!-- AOS Script -->
  <script src="../aos.config.js"></script>
</html>
